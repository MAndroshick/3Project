<h1>Doctors#edit</h1>
<p>Find me in app/views/doctors/edit.html.erb</p>
<%= form_tag(:action => 'edit_submit', :id => @editDoctor[:id]) do %>
<p>First Name:</br><%= text_field_tag(:first_name, @editDoctor[:name_first], :required => true) %></p>
<p>Last Name:</br><%= text_field_tag(:last_name, @editDoctor[:name_last], :required => true) %></p>
<p>Specialty:</br><%= text_field_tag(:specialty, @editDoctor[:specialty], :required => true) %></p>
<%= submit_tag("Save Changes")%>
<% end %>